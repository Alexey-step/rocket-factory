{
  "id": null,
  "title": "Order Service Overview",
  "description": "Основные метрики Order http сервиса",
  "tags": ["order", "http", "microservice"],
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "annotations": {
    "list": []
  },
  "templating": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Total Orders Count",
      "description": "Количество созданных заказов",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "orders_total",
          "legendFormat": "{{method}} - {{status}}",
          "datasource": {
            "type": "prometheus",
            "uid": "aex9w9dqvarcwa"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 2,
      "title": "Total Orders Revenue",
      "description": "Суммарная выручка по заказам",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "orders_revenue_total",
          "legendFormat": "{{method}} - {{status}}",
          "datasource": {
            "type": "prometheus",
            "uid": "aex9w9dqvarcwa"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyRUB",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 3,
      "title": "Request Rate",
      "description": "Количество запросов в секунду к Order сервису",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "rate(order_requests_total[1m])",
          "legendFormat": "{{method}} - {{status}}",
          "datasource": {
            "type": "prometheus",
            "uid": "aex9w9dqvarcwa"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 4,
      "title": "Request Duration p95",
      "description": "95-й перцентиль времени выполнения запросов",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(order_request_duration_seconds_bucket[1m])) by (le, method))",
          "legendFormat": "{{method}} - p95",
          "datasource": {
            "type": "prometheus",
            "uid": "aex9w9dqvarcwa"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    }
  ],
  "version": 1,
  "uid": "order-service-overview",
  "schemaVersion": 30
}